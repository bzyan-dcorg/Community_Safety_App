VITE_API_BASE_URL=http://localhost:8000
VITE_API_PORT=8000

# Google OAuth (Web)
VITE_GOOGLE_CLIENT_ID=YOUR_GOOGLE_OAUTH_CLIENT_ID.apps.googleusercontent.com

# Apple OAuth (Web)
VITE_APPLE_CLIENT_ID=com.yourteam.communityapp.service
VITE_APPLE_REDIRECT_URI=https://yourdomain.example.com/api/auth/apple/callback

# Map tiles (override if your region blocks OpenStreetMap CDNs)
VITE_MAP_TILE_URL=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png
VITE_MAP_TILE_ATTRIBUTION=&copy; OpenStreetMap contributors
VITE_MAP_MAX_ZOOM=19
